<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <view n="100">
    <name>Copilot View</name>
    <internal archive="y">true</internal>
    <type>lookfrom</type>
    <config>
      <from-model type="bool">true</from-model>
      <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
      <heading-offset-deg type="double">0</heading-offset-deg> <!-- inside its window -->
      <x-offset-m archive="y">  0.393 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  1.184 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -8.493 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> 0 </pitch-offset-deg>
      <default-field-of-view-deg type="double"> 65 </default-field-of-view-deg>
    </config>
  </view>

